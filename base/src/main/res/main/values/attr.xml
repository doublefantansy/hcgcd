<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PickerView">
        <attr name="pv_text_size" format="dimension" />
        <attr name="pv_text_color" format="color" />
        <attr name="pv_number" format="integer" />
        <attr name="pv_line_height" format="dimension" />
        <attr name="pv_line_color" format="color" />
    </declare-styleable>
    <declare-styleable name="IndulgeTitle">
        <attr format="boolean" name="showLeftBack"></attr>
        <attr format="string" name="centerText"></attr>
        <attr format="reference" name="rightImage"></attr>
        <attr format="reference" name="leftImage"></attr>
        <attr format="reference" name="mostLeftImage"></attr>
        <attr format="reference" name="centerTextColor"></attr>
    </declare-styleable>
    <declare-styleable name="MainItemLayout">
        <attr format="integer" name="rowCounts"></attr>
        <attr format="integer" name="countsEachRow"></attr>
    </declare-styleable>
    <declare-styleable name="RefreshAndLoadMoreLayout">
        <attr name="isGrid" format="boolean"></attr>
        <attr name="gridCount" format="integer"></attr>
    </declare-styleable>
</resources>